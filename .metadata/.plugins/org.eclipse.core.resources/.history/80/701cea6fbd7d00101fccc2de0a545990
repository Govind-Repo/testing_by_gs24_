package wipro;

public class Array {

	public static int[] updatedArray(int []array1,int []array2) {
		return array2;
		
	}
	
	public static void main(String[] args) {

		int[] array1 = new int[] {1,3,6};
		int[] array2 = new int[] {4,5};
		
		updatedArray(array1, array2);
	    int size=array1.length+array2.length;
	}

