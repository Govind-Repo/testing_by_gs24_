		//Input: AabbbCccccdddd
		//Output: A2b3C5d4
		
		String charChain = "AabbbCccccdddd";
		int count=1;
		String chaChainUpdated = charChain.toUpperCase();
		for(int i=0;i<chaChainUpdated.length();i++) {
			
			for(int j=i+1;j<chaChainUpdated.length();j++) {
				
				if(chaChainUpdated.charAt(i)==chaChainUpdated.charAt(j)) {
					count++;
				}
			}
		}
		
		
				
	}
}
